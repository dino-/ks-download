name: ks-download
version: 1.19
cabal-version: >=1.8
build-type: Custom
license: BSD3
license-file: LICENSE
copyright: 2014-2015 Dino Morelli
maintainer: Dino Morelli <dino@ui3.info>
stability: experimental
homepage: http://hub.darcs.net/dino/ks-download
synopsis: Data downloader and parser for the KitchenSnitch project
description:
    Bar Baz
category: Unknown
author: Dino Morelli
tested-with: GHC >=7.10.1
data-files:
    resources/ks-download.conf
    resources/ks-download-nc_chatham.conf
    resources/ks-download-nc_durham.conf
    resources/ks-download-nc_orange.conf
    resources/ks-download-nc_wake.conf
    resources/mongodb.conf
extra-source-files:
    changelog.md
    doc/dev/notes
    README.md
    util/gentags.sh
    util/inspconv.coffee
    util/install.sh
    util/ks-dl-nightly.sh
    util/resources/geocode.json
    util/resources/nc-wake/searchResultPage.html
    util/resources/places.json

source-repository head
    type: darcs
    location: http://hub.darcs.net/dino/ks-download

executable ks-dlinsp-cdp
    main-is: ks-dlinsp-cdp.hs
    build-depends:
        aeson ==0.9.*,
        base >=3 && <5,
        bytestring >=0.10.6.0 && <0.11,
        containers >=0.5.6.2 && <0.6,
        exceptions >=0.8.2.1 && <0.9,
        filepath >=1.4.0.0 && <1.5,
        HTTP >=4000.2.23 && <4000.3,
        ks-library >=1.9,
        lens >=4.13.2.1 && <4.14,
        lens-aeson >=1.0.0.5 && <1.1,
        mtl >=2.2.1 && <2.3,
        regex-compat >=0.95.1 && <0.96,
        tagsoup >=0.13.8 && <0.14,
        tce-conf ==1.1.*,
        text >=1.2.1.3 && <1.3,
        time >=1.5.0.1 && <1.6,
        utf8-string >=1.0.1.1 && <1.1,
        wreq >=0.4.1.0 && <0.5
    hs-source-dirs: src
    other-modules:
        KS.DLInsp.CDP.Downloader
        KS.DLInsp.CDP.Opts
        KS.DLInsp.CDP.Types
        KS.SourceConfig
        KS.Util
    ghc-options: -Wall

executable ks-dlinsp-nc_wake
