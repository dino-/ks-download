        mtl >=2.2.1 && <2.3,
        regex-compat >=0.95.1 && <0.96,
        tagsoup >=0.13.8 && <0.14,
        tce-conf ==1.1.*,
        text >=1.2.1.3 && <1.3,
        time >=1.5.0.1 && <1.6,
        utf8-string >=1.0.1.1 && <1.1
    hs-source-dirs: src
    other-modules:
        KS.DLInsp.NCWake.Downloader
